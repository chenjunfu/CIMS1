<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts PUBLIC
 "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
 "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="cultureman" extends="base" namespace="/cultureman">
		<!-- 专业表 -->
		<action name="Major" class="com.yun.ui.action.cultureman.Major">
			<result name="MajorDetails">/cultureman/MajorDetails.jsp</result>
			<result name="MajorEdit">/cultureman/MajorEdit.jsp</result>
			<result name="MajorList">/cultureman/MajorList.jsp</result>
		</action>
		<!-- 文化人才表 -->
		<action name="Talent" class="com.yun.ui.action.cultureman.Talent">
			<result name="TalentDetails">/cultureman/TalentDetails.jsp</result>
			<result name="TalentEdit">/cultureman/TalentEdit.jsp</result>
			<result name="TalentList">/cultureman/TalentList.jsp</result>
		</action>
		<!-- 已有成果表 -->
		<action name="TalentAchievement" class="com.yun.ui.action.cultureman.TalentAchievement">
			<result name="TalentAchievementDetails">/cultureman/TalentAchievementDetails.jsp</result>
			<result name="TalentAchievementEdit">/cultureman/TalentAchievementEdit.jsp</result>
			<result name="TalentAchievementList">/cultureman/TalentAchievementList.jsp</result>
		</action>
		<!-- 专长表 -->
		<action name="TalentSkills" class="com.yun.ui.action.cultureman.TalentSkills">
			<result name="TalentSkillsDetails">/cultureman/TalentSkillsDetails.jsp</result>
			<result name="TalentSkillsEdit">/cultureman/TalentSkillsEdit.jsp</result>
			<result name="TalentSkillsList">/cultureman/TalentSkillsList.jsp</result>
		</action>

	</package>

</struts>
